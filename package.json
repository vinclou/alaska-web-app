{
  "name": "alaska-web-app",
  "version": "1.0.0",
  "description": "Web app for the research project",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js --ignore",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "nodemonConfig": {
    "ignore": [
      "docs/*"
    ],
    "delay": "1500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinclou/alaska-web-app.git"
  },
  "keywords": [
    "react",
    "mongo",
    "express",
    "node",
    "docker"
  ],
  "author": "Vadzim Arlou <totsamiman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vinclou/alaska-web-app/issues"
  },
  "homepage": "https://github.com/vinclou/alaska-web-app#readme",
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.1.0",
    "config": "^3.2.6",
    "express": "^4.18.2",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^1.1.0",
    "mongoose": "^5.9.2",
    "react-animated-tree": "^1.0.10",
    "react-map-gl": "^5.2.5",
    "react-markdown": "^4.3.1",
    "recharts": "^1.8.5",
    "walk": "^2.3.14"
  }
}
